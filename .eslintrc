{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "7",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "jest": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "semi": ["error","never"],
    "no-console": "off",
    "no-mixed-spaces-and-tabs": "off"
  }
}